<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">
    <SolidColorBrush x:Key="PrimaryColorBrush"
                     Color="#48578E"></SolidColorBrush>
    <SolidColorBrush x:Key="SecondaryColorBrush"
                     Color="#7D8EC4"></SolidColorBrush>
    <SolidColorBrush x:Key="TertiaryColorBrush"
                     Color="#9DADDF"></SolidColorBrush>
    <SolidColorBrush x:Key="SelectedColorBrush"
                     Color="AliceBlue"></SolidColorBrush>
    <SolidColorBrush x:Key="LightColorBrush"
                     Color="#C0CFFA"></SolidColorBrush>
    <SolidColorBrush x:Key="DarkColorBrush"
                     Color="#334173"></SolidColorBrush>

    <sys:Double x:Key="FontSizeXS">11</sys:Double>
    <sys:Double x:Key="FontSizeS">14</sys:Double>
    <sys:Double x:Key="FontSizeM">16</sys:Double>
    <sys:Double x:Key="FontSizeL">18</sys:Double>
    <sys:Double x:Key="FontSizeXL">24</sys:Double>
    <sys:Double x:Key="FontSizeXXL">36</sys:Double>
    <sys:Double x:Key="FontSize3XL">48</sys:Double>
</ResourceDictionary>